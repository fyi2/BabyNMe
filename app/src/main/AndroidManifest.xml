<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sherman.android.babynme">

    <application
        android:name=".BabyNMeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activites.MainActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activites.SettingsActivity"
            android:parentActivityName=".Activites.MainActivity" />
        <activity android:name=".Activites.TestActivity" />
        <activity android:name=".Activites.WelcomeActivity" />
        <activity android:name=".Activites.HomeActivity" />
        <activity android:name=".Activites.SleepTrackerActivity"></activity>
    </application>

</manifest>